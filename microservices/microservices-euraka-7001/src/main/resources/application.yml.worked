server: 
  port: 7001
 
eureka: 
  instance:
    hostname: eureka7001.com #eureka server name
  client: 
    register-with-eureka: false     #false indicate don't register eureka itself to Eureka instance
    fetch-registry: false     #false means myself is register, I don't need to register
    service-url: 
      defaultZone: http://eureka7001.com:7001/eureka/